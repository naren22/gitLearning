<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="nt:unstructured" jcr:title="User Profile Form"
	sling:resourceType="cq/gui/components/authoring/dialog"
	maximized="{Boolean}true">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/coral/foundation/container">
		<items jcr:primaryType="nt:unstructured">
			<tabs jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/tabs"
				maximized="{Boolean}true">
				<items jcr:primaryType="nt:unstructured">
					<tab-1 jcr:primaryType="nt:unstructured"
						jcr:title="Profile Form Setup"
						sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<column jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<formHeader jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Form Header label" emptyText="ALL ABOUT YOU"
										name="./formHeader" wrapperClass="cmp-image--editor-alt" />
									<saveChangesButton
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Form Submit Button label" emptyText="Save Changes"
										name="./saveChangesButton"
										wrapperClass="cmp-image--editor-alt" />
									<titleAndName jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Title And Full Name field label"
										emptyText="Title And Name" name="./titleAndName"
										wrapperClass="cmp-image--editor-alt" />
									<title jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Title field label" emptyText="Title"
										name="./title" wrapperClass="cmp-image--editor-alt" />
									<firstName jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="First Name field label" emptyText="First Name"
										name="./firstName" wrapperClass="cmp-image--editor-alt" />
									<lastName jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Last Name field label" emptyText="Last Name"
										name="./lastName" wrapperClass="cmp-image--editor-alt" />
									<phone jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Phone field label" name="./phone"
										emptyText="Phone Number" wrapperClass="cmp-image--editor-alt" />
									<phoneph jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Phone field placeholder" name="./phonePh"
										emptyText="Phone Number (Optional)"
										wrapperClass="cmp-image--editor-alt" />
									<phoneSubtext jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Phone input field subtext" name="./phoneSubtext"
										emptyText="Begin with 00, followed by your country code, then your phone number"
										wrapperClass="cmp-image--editor-alt" />
									<organization jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Organization field label" name="./organization"
										emptyText="Company" wrapperClass="cmp-image--editor-alt" />
									<relationship jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Relationship field label" name="./relationship"
										emptyText="Relationship to Gore"
										wrapperClass="cmp-image--editor-alt" />
									<jobTitle jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Job Title field label" name="./jobTitle"
										emptyText="Job Title" wrapperClass="cmp-image--editor-alt" />
									<companyAddress
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Company Address field label"
										name="./companyAddress" emptyText="Company Address"
										wrapperClass="cmp-image--editor-alt" />
									<address1 jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Address1 field label" name="./address1"
										emptyText="Street" wrapperClass="cmp-image--editor-alt" />
									<city jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="City field label" name="./city" emptyText="City"
										wrapperClass="cmp-image--editor-alt" />
									<zip jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Zip code field label" name="./zip"
										emptyText="Postal Code" wrapperClass="cmp-image--editor-alt" />
									<country jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Country field label" name="./country"
										emptyText="Country" wrapperClass="cmp-image--editor-alt" />
									<acccountSettings
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Account Settings label" name="./acccountSettings"
										emptyText="Account Settings"
										wrapperClass="cmp-image--editor-alt" />
									<email jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Email field label" name="./email"
										emptyText="Email" wrapperClass="cmp-image--editor-alt" />
									<emailPrefix jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Email Prefix" name="./emailPrefix"
										emptyText="Your email address is "
										wrapperClass="cmp-image--editor-alt" />
									<pwd jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Password label" name="./pwd" emptyText="Password"
										wrapperClass="cmp-image--editor-alt" />
									<pwdPh jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Password field label" name="./pwdLbl"
										emptyText="Current Password"
										wrapperClass="cmp-image--editor-alt" />
									<newpwd jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="New Password field label" name="./newpwd"
										emptyText="New Password" wrapperClass="cmp-image--editor-alt" />
									<passwordhint jcr:primaryType="nt:unstructured"
										sling:resourceType="cq/gui/components/authoring/dialog/richtext"
										fieldLabel="Password Requirements"
										name="./passwordRequirements"
										useFixedInlineToolbar="{Boolean}true">
										<rtePlugins jcr:primaryType="nt:unstructured">
											<format jcr:primaryType="nt:unstructured"
												features="bold,italic" />
											<justify jcr:primaryType="nt:unstructured"
												features="-" />
											<links jcr:primaryType="nt:unstructured"
												features="modifylink,unlink" />
											<lists jcr:primaryType="nt:unstructured" features="*" />
											<tracklinks jcr:primaryType="nt:unstructured"
												features="*" />
										</rtePlugins>
										<uiSettings jcr:primaryType="nt:unstructured">
											<cui jcr:primaryType="nt:unstructured">
												<inline jcr:primaryType="nt:unstructured"
													toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
													<popovers jcr:primaryType="nt:unstructured">
														<justify jcr:primaryType="nt:unstructured"
															items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
															ref="justify" />
														<lists jcr:primaryType="nt:unstructured"
															items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
															ref="lists" />
														<paraformat jcr:primaryType="nt:unstructured"
															items="paraformat:getFormats:paraformat-pulldown"
															ref="paraformat" />
													</popovers>
												</inline>
												<dialogFullScreen
													jcr:primaryType="nt:unstructured"
													toolbar="[format#bold,format#italic,format#underline,justify#justifyleft,justify#justifycenter,justify#justifyright,lists#unordered,lists#ordered,lists#outdent,lists#indent,links#modifylink,links#unlink,table#createoredit,#paraformat,image#imageProps]">
													<popovers jcr:primaryType="nt:unstructured">
														<paraformat jcr:primaryType="nt:unstructured"
															items="paraformat:getFormats:paraformat-pulldown"
															ref="paraformat" />
													</popovers>
												</dialogFullScreen>
												<tableEditOptions
													jcr:primaryType="nt:unstructured"
													toolbar="[table#insertcolumn-before,table#insertcolumn-after,table#removecolumn,-,table#insertrow-before,table#insertrow-after,table#removerow,-,table#mergecells-right,table#mergecells-down,table#mergecells,table#splitcell-horizontal,table#splitcell-vertical,-,table#selectrow,table#selectcolumn,-,table#ensureparagraph,-,table#modifytableandcell,table#removetable,-,undo#undo,undo#redo,-,table#exitTableEditing,-]" />
											</cui>
										</uiSettings>
									</passwordhint>
									<passwordChangeSuccess
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Password change success message"
										name="./passwordChangeSuccess"
										emptyText="Password changed successfully"
										wrapperClass="cmp-image--editor-alt" />
									<passwordMismatch
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Password mismatch message"
										name="./passwordMismatch"
										emptyText="Doesn’t match current password"
										wrapperClass="cmp-image--editor-alt" />
									<passwordChangeFail
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Password change failure message"
										name="./passwordChangeFail"
										emptyText="Please try again. Your password doesn't fulfil the requirements below."
										wrapperClass="cmp-image--editor-alt" />

									<forgotPassword
										jcr:primaryType="nt:unstructured"
										sling:resourceType="cq/gui/components/authoring/dialog/richtext"
										fieldLabel="Forgot Password Text" name="./forgotPwdTxt"
										useFixedInlineToolbar="{Boolean}true">
										<rtePlugins jcr:primaryType="nt:unstructured">
											<format jcr:primaryType="nt:unstructured"
												features="bold,italic" />
											<justify jcr:primaryType="nt:unstructured"
												features="-" />
											<links jcr:primaryType="nt:unstructured"
												features="modifylink,unlink" />
											<lists jcr:primaryType="nt:unstructured" features="*" />
											<tracklinks jcr:primaryType="nt:unstructured"
												features="*" />
										</rtePlugins>
										<uiSettings jcr:primaryType="nt:unstructured">
											<cui jcr:primaryType="nt:unstructured">
												<inline jcr:primaryType="nt:unstructured"
													toolbar="[format#bold,format#italic,format#underline,#justify,#lists,links#modifylink,links#unlink,#paraformat]">
													<popovers jcr:primaryType="nt:unstructured">
														<justify jcr:primaryType="nt:unstructured"
															items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
															ref="justify" />
														<lists jcr:primaryType="nt:unstructured"
															items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
															ref="lists" />
														<paraformat jcr:primaryType="nt:unstructured"
															items="paraformat:getFormats:paraformat-pulldown"
															ref="paraformat" />
													</popovers>
												</inline>
												<dialogFullScreen
													jcr:primaryType="nt:unstructured"
													toolbar="[format#bold,format#italic,format#underline,justify#justifyleft,justify#justifycenter,justify#justifyright,lists#unordered,lists#ordered,lists#outdent,lists#indent,links#modifylink,links#unlink,table#createoredit,#paraformat,image#imageProps]">
													<popovers jcr:primaryType="nt:unstructured">
														<paraformat jcr:primaryType="nt:unstructured"
															items="paraformat:getFormats:paraformat-pulldown"
															ref="paraformat" />
													</popovers>
												</dialogFullScreen>
												<tableEditOptions
													jcr:primaryType="nt:unstructured"
													toolbar="[table#insertcolumn-before,table#insertcolumn-after,table#removecolumn,-,table#insertrow-before,table#insertrow-after,table#removerow,-,table#mergecells-right,table#mergecells-down,table#mergecells,table#splitcell-horizontal,table#splitcell-vertical,-,table#selectrow,table#selectcolumn,-,table#ensureparagraph,-,table#modifytableandcell,table#removetable,-,undo#undo,undo#redo,-,table#exitTableEditing,-]" />
											</cui>
										</uiSettings>
									</forgotPassword>
									<languageTitle jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Language Title" name="./languageTitle"
										emptyText="Language" wrapperClass="cmp-image--editor-alt" />
									<currLanguagePrefix
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Current Language Prefix"
										name="./currLanguagePrefix"
										emptyText="Your language is currently set to"
										wrapperClass="cmp-image--editor-alt" />
									<languageSubtext
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Language Subtext" name="./languageSubtext"
										emptyText="Choose the language you'd like to use for this platform."
										wrapperClass="cmp-image--editor-alt" />
									<languageSelect
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Language Select Label" name="./languageSelect"
										emptyText="Portal Language"
										wrapperClass="cmp-image--editor-alt" />
								</items>
							</column>
						</items>
					</tab-1>
					<tab-2 jcr:primaryType="nt:unstructured" jcr:title="Messages"
						sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<column jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<nameFormSuccessMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Name Update Success Message" emptyText="Your Name is updated successfully."
										name="./nameFormSuccessMessage" />
									<nameFormErrorMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Name Update Error Message" emptyText="Error Occurred while updating your name."
										name="./nameFormErrorMessage" />
									<phoneFormSuccessMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Phone Number Update Success Message" emptyText="Your Phone Number is updated successfully."
										name="./phoneFormSuccessMessage" />
									<phoneFormErrorMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Phone Number Update Error Message" emptyText="Error Occurred while updating your phone number."
										name="./phoneFormErrorMessage" />
									<jobTitleFormSuccessMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Job Title Update Success Message" emptyText="Your Job Title is updated successfully."
										name="./jobTitleFormSuccessMessage" />
									<jobTitleFormErrorMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Job Title Update Error Message" emptyText="Error Occurred while updating your job title number."
										name="./jobTitleFormErrorMessage" />
									<addressFormSuccessMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Address Update Success Message" emptyText="Your Address is updated successfully."
										name="./addressFormSuccessMessage" />
									<addressFormErrorMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Address Update Error Message" emptyText="Error Occurred while updating your address"
										name="./addressFormErrorMessage" />
									<passwordFormSuccessMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Password Update Success Message" emptyText="Your Password is updated successfully."
										name="./passwordFormSuccessMessage" />
									<passwordFormErrorMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Password Update Error Message" emptyText="Error Occurred while updating your password."
										name="./passwordFormErrorMessage" />
									<languageFormSuccessMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Language Update Success Message" emptyText="Your Language Preference is updated successfully."
										name="./languageFormSuccessMessage" />
									<languageFormErrorMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Language Update Error Message" emptyText="Error Occurred while updating your language preference."
										name="./languageFormErrorMessage" />
								</items>
							</column>
						</items>
					</tab-2>
					<tab-3 jcr:primaryType="nt:unstructured"
						jcr:title="Delete Account Form Setup"
						sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<column jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<deleteFormHeader
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Delete Form Header" emptyText="Delete Account"
										name="./deleteFormHeader" wrapperClass="cmp-image--editor-alt" />
									<deleteButton jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Delete Button label" emptyText="Delete Account"
										name="./deleteButton" wrapperClass="cmp-image--editor-alt" />
									<deleteFormFind
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Delete reasons option 1"
										emptyText="I didn't find what i was looking for"
										name="./deleteFormFind" wrapperClass="cmp-image--editor-alt" />
									<deleteFormUseful
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Delete reasons option 2" emptyText="Not useful"
										name="./deleteFormUseful" wrapperClass="cmp-image--editor-alt" />
									<deleteFormOther
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Delete reasons option 3" emptyText="Other"
										name="./deleteFormOther" wrapperClass="cmp-image--editor-alt" />
									<deleteReasonHeader
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Delete message header"
										emptyText="Your feedback matters. Help us improve this platform."
										name="./deleteReasonHeader"
										wrapperClass="cmp-image--editor-alt" />
									<deleteReasonText
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Delete message subheader"
										emptyText="Write your message..." name="./deleteReasonText"
										wrapperClass="cmp-image--editor-alt" />
									<deleteRecipientEmail
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Delete Email Receiver Address"
										name="./deleteRecipientEmail"
										wrapperClass="cmp-image--editor-alt" />
									<deleteFormSuccessMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Delete Account Success Message" emptyText="Your Delete Account request is sent successfully."
										name="./deleteFormSuccessMessage" />
									<deleteFormErrorMessage
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Delete Account Error Message" emptyText="Error Occurred while updating your delete preference."
										name="./deleteFormErrorMessage" />
								</items>
							</column>
						</items>
					</tab-3>
				</items>
			</tabs>
		</items>
	</content>
</jcr:root>
